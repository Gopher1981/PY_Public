<script language="VBScript"> 
//Window.ReSizeTo 240, 250 

Sub ONETAGType()
Set FileSysObj = CreateObject("Scripting.FileSystemObject")
////Set ObjWshNet = CreateObject("WScript.Network")
////net.AddPrinterConnection "LPT7","\\10.151.53.22\rug-cfg-zebra-07"
////Set WshShell = CreateObject("Wscript.Shell")
Set ObjZebra = FileSysObj.CreateTextFile("lpt7", True) 
TagSku = Tag.TagSku.value
     
          ZPLText = "^XA"
          ZPLText = ZPLText & "^LH05,0"
          ZPLText = ZPLText & "^FO1,20"
	  ZPLText = ZPLText & "^AsN,25,25"
          ZPLText = ZPLText & "^FD"
          ZPLText = ZPLText & "Device Asset Tag"

          ZPLText = ZPLText & "^FS"
          ZPLText = ZPLText & "^FO03,60^BY2.1"
          ZPLText = ZPLText & "^B3N,N,100,Y,N"        
          ZPLText = ZPLText & "^FD"
          ZPLText = ZPLText & TagSku
          ZPLText = ZPLText & "^FS"
          ZPLText = ZPLText & "^XZ"
          ObjZebra.Write(ZPLText)
          objZebra.Close
          Set objZebra = Nothing
    Tag.TagSku.value = ""    
    Tag.TagSku.Focus   
   
End Sub

Sub SerialType()
Set FileSysObj = CreateObject("Scripting.FileSystemObject")
////Set ObjWshNet = CreateObject("WScript.Network")
////net.AddPrinterConnection "LPT7","\\10.151.53.22\rug-cfg-zebra-07"
////Set WshShell = CreateObject("Wscript.Shell")
Set ObjZebra = FileSysObj.CreateTextFile("lpt7", True)
SERIALSku = SER.SERIALSku.value
     
          ZPLText = "^XA"
          ZPLText = ZPLText & "^LH05,0"
          ZPLText = ZPLText & "^FO1,20"
	  ZPLText = ZPLText & "^AsN,25,25"
          ZPLText = ZPLText & "^FD"
          ZPLText = ZPLText & "Serial Number"

          ZPLText = ZPLText & "^FS"
          ZPLText = ZPLText & "^FO03,60^BY2.1"
          ZPLText = ZPLText & "^B3N,N,100,Y,N"        
          ZPLText = ZPLText & "^FD"
          ZPLText = ZPLText & SERIALSku
          ZPLText = ZPLText & "^FS"
          ZPLText = ZPLText & "^XZ"
          ObjZebra.Write(ZPLText)
          objZebra.Close
          Set objZebra = Nothing
    SER.SERIALSku.value = "" 
    SER.SERIALSku.focus       
   
End Sub

Sub RANGETag() 
Set FileSysObj = CreateObject("Scripting.FileSystemObject")
        Set ObjZebra = FileSysObj.CreateTextFile("LPT7", True)
        Set WshShell = CreateObject("WScript.Shell")
        Set wsh = CreateObject("WScript.Shell")
        
Prefix = RTag.Prefix.value
Prefix = RTAG.Prefix.value
Suffix = RTAG.Suffix.value
Start = RTAG.Start.value 
Last = RTAG.Last.value
do
          ZPLText = "^XA"
          ZPLText = ZPLText & "^LH10,5"
          ZPLText = ZPLText & "^FO120,0"
          ZPLText = ZPLText & "^AsN,20,20"
          ZPLText = ZPLText & "^FD"
          ZPLText = ZPLText & "Device Asset Tag"
          ZPLText = ZPLText & "^FS"
          ZPLText = ZPLText & "^FO03,40"
          ZPLText = ZPLText & "^B3N,N,95,Y,N"
          ZPLText = ZPLText & "^FD"
          ZPLText = ZPLText & Prefix&Start&Suffix 
         ZPLText = ZPLText & "^FS"
          ZPLText = ZPLText & "^XZ"
               ObjZebra.Write(ZPLText)

Start = Start + 1
Loop Until Start > 0 + Last
				objZebra.Close
				Set objZebra = Nothing
    	RTAG.Prefix.value = ""
   		RTAG.Suffix.value = ""
    	RTAG.Start.value = ""
        RTAG.Last.value ="" 
        RTAG.Prefix.focus
end Sub

Sub BATTERYType()
Set FileSysObj = CreateObject("Scripting.FileSystemObject")
////Set ObjWshNet = CreateObject("WScript.Network")
////net.AddPrinterConnection "LPT7","\\10.151.53.22\rug-cfg-zebra-07"
////Set WshShell = CreateObject("Wscript.Shell")
Set ObjZebra = FileSysObj.CreateTextFile("lpt7", True)
INTSku = Bat.INTSku.value
     
          ZPLText = "^XA"
          ZPLText = ZPLText & "^LH05,0"
          ZPLText = ZPLText & "^FO1,20"
	      ZPLText = ZPLText & "^AsN,47,47"
          ZPLText = ZPLText & "^FD"
          ZPLText = ZPLText & "    THIS UNIT HAS A LITHIUM/ION"
		  ZPLText = ZPLText & "^FS"
	      ZPLText = ZPLText & "^FO1,60"
	      ZPLText = ZPLText & "^AsN,47,47"
          ZPLText = ZPLText & "^FD"
          ZPLText = ZPLText & "            BATTERY FITTED"
		  ZPLText = ZPLText & "^FS"
		  	      ZPLText = ZPLText & "^FO1,120"
	      ZPLText = ZPLText & "^AsN,47,47"
          ZPLText = ZPLText & "^FD"
          ZPLText = ZPLText & "  SIGNED:       "& INTSku
		  ZPLText = ZPLText & "^FS"
          ZPLText = ZPLText & "^XZ"
          ObjZebra.Write(ZPLText)
          objZebra.Close
          Set objZebra = Nothing
    Bat.INTSku.value = ""   
    bat.INTSku.focus     
   
End Sub

</script>

